{
    "name": "lf-cli",
    "description": "The ListenFirst API Command Line Interface",
    "version": "0.0.0",
    "author": "Mike Stanley",
    "bin": {
        "lf-cli": "./bin/run"
    },
    "bugs": "https://github.com/ListenFirstMedia/lf-cli/issues",
    "dependencies": {
        "@iarna/toml": "^2.2.5",
        "@oclif/command": "^1.7.0",
        "@oclif/config": "^1.16.0",
        "@oclif/plugin-help": "^3.1.0",
        "@types/inquirer": "^6.5.0",
        "@types/listr": "^0.14.2",
        "@types/lodash": "^4.14.157",
        "@types/node-fetch": "^2.5.7",
        "cli-ux": "^5.4.9",
        "inquirer": "^7.3.2",
        "listr": "^0.14.3",
        "lodash": "^4.17.19",
        "moment": "^2.27.0",
        "node-fetch": "^2.6.0",
        "toml": "^3.0.0",
        "tslib": "^1.13.0"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.22.2",
        "@types/jest": "^26.0.5",
        "@types/node": "^10.17.27",
        "@types/prettier": "^2.0.2",
        "eslint": "^5.16.0",
        "eslint-config-oclif": "^3.1.0",
        "eslint-config-oclif-typescript": "^0.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "globby": "^10.0.2",
        "husky": "^4.2.5",
        "jest": "^26.1.0",
        "lint-staged": "^10.2.11",
        "prettier": "^2.0.5",
        "ts-jest": "^26.1.3",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.6"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/ListenFirstMedia/lf-cli",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "lf-cli",
        "plugins": [
            "@oclif/plugin-help"
        ],
        "topics": {
            "auth": {
                "description": "Authentication related commands"
            },
            "config": {
                "description": "Manage lf-cli configuration profiles"
            },
            "datasets": {
                "description": "Query the ListenFirst Data Dictionary"
            },
            "analytics": {
                "description": "Produce and submit analytical queries"
            },
            "brand-views": {
                "description": "Query available Brand Views"
            },
            "brand-view-sets": {
                "description": "Query avaible Brand View Sets"
            }
        },
        "s3": {
            "bucket": "dist.lfm",
            "host": "https://dist.listenfirstmedia.com"
        }
    },
    "repository": "ListenFirstMedia/lf-cli",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "jest",
        "version": "oclif-dev readme && git add README.md",
        "format": "prettier --write src/",
        "lint": "eslint --fix --ext ts src/"
    },
    "types": "lib/index.d.ts",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,js}": [
            "npx prettier --check",
            "eslint --fix-dry-run --ext ts src"
        ]
    }
}
